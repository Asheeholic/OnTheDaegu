<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.otd.faq.service.FaqMapper">
	
	<select id="faqRentalOffice" resultType="co.yedam.otd.faq.vo.FaqVO">
		select * from faq where faq_category = '대여소'
	</select>
	
	<select id="faqPaymentRefundMileage" resultType="co.yedam.otd.faq.vo.FaqVO">
		select * from faq where faq_category = '결제/환불/마일리지'
	</select>
	
	<select id="faqUseInformation" resultType="co.yedam.otd.faq.vo.FaqVO">
		select * from faq where faq_category = '이용안내'
	</select>
	
	<select id="faqBicycleFacilityManagement" resultType="co.yedam.otd.faq.vo.FaqVO">
		select * from faq where faq_category = '자전거 및 시설관리'
	</select>
	
	<select id="faqRentalReturn" resultType="co.yedam.otd.faq.vo.FaqVO">
		select * from faq where faq_category = '대여/반납'
	</select>				

</mapper>